def adjacency_check(matrix, i, j):
    if i >= len(matrix) or j >= len(matrix): return "Incorrect index"
    return bool(matrix[i][j])
a = [[0, 1, 0],
     [1, 0, 0],
     [0, 0, 0]]
adjacency_check(a, 1, 3)
'Incorrect index'
