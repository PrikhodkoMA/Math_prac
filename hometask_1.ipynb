import math
def snakefill(n):
    return int((math.log(n * n, 2)) // 1)
555
snakefill(555)
18
def oddishOrEvenish(x):
    s = 0
    while x > 0:
        s += x % 10
        x = x // 10
    if s % 2: 
        print("Oddish")
    else: 
        print("Evenish")
